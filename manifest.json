{
  "manifest_version": 3,
  "name": "Chrome Tabs Removal",
  "version": "1.0",
  "description": "Highlight and manage tabs based on RAM usage.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
